// Fibonacci function test
extern println 1
extern get_value 0

func fib
{
	// Parameter x
	param x
	
	pushv x
	calle println
	
	// jump if x <= 1
	pushv x
	pushi 1
	jle end

	// x > 1, call fib recursive
	// return fib (x - 1) + fib (x - 2)
	
	// (x - 2)
	pushv x
	pushi 2
	sub
	
	// fib(x - 2)
	call fib
	
	// (x - 1)
	pushv x
	pushi 1
	sub
	
	// fib(x - 1)
	call fib
	
	// fib(x - 1) + fib(x - 2)
	add
	
	// return fib (x - 1) + fib (x - 2)
	popv x
	
	// x <= 1, return x
	label end
	retv x
}

func main
{
	var x
	// Retrieve value from host
	calle get_value
	popv x
	pushv x
	calle println
	
	pushv x
	call fib
	// Result on stack
	calle println
}
