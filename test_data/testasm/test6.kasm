extern println 1

// Test for calling convention with return value

func a
{
	param x // Parameter should, be 10
	calle println // Print 10
	reti 20 // Return value
}

func main
{
	// Push 10 on stack
	pushi 10
	call a // Expects single parameter
	// Return value on stack
	calle println // Should print 20
	ret
}
